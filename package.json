{
  "name": "redux-chillout",
  "version": "1.0.1",
  "license": "MIT",
  "description": "Redux utility for prevent boilerplate code",
  "author": "Alan Saracho <saracho.alan@gmail.com>",
  "main": "lib/index.js",
  "module": "es/index.js",
  "typings": "./index.d.ts",
  "dependencies": {
    "lodash": "^4.17.11"
  },
  "scripts": {
    "clean": "rm -rf lib",
    "build": "babel src --out-dir lib",
    "browserify": "mkdir -p dist && browserify -s ReduxChillout lib/index.js > dist/redux-chillout.js",
    "minify": "terser dist/redux-chillout.js -o dist/redux-chillout.min.js",
    "prepublish": "npm run clean && npm run build && npm run browserify && npm run minify"

  },
  "devDependencies": {
    "@babel/cli": "7.4.4",
    "@babel/core": "7.4.5",
    "@babel/node": "7.4.5",
    "@babel/plugin-proposal-object-rest-spread": "7.4.4",
    "@babel/plugin-transform-object-assign": "7.2.0",
    "@babel/preset-env": "7.4.5",
    "@babel/register": "7.4.4",
    "browserify": "16.2.3",
    "chai": "^3.2.0",
    "cross-env": "^1.0.7",
    "eslint": "^4.18.2",
    "eslint-config-airbnb": "1.0.2",
    "eslint-plugin-react": "^4.1.0",
    "mocha": "^2.2.5",
    "terser": "4.0.0",
    "typescript": "3.4.5"
  },
  "peerDependencies": {
    "lodash": "^4.17.11"
  }
}
